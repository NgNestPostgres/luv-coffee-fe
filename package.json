{
  "name": "@ngnestpostgres/luv-coffee-fe",
  "version": "0.0.7",
  "private": true,
  "scripts": {
    "ng": "ng",
    "serve:local": "ng serve",
    "serve:dev": "ng serve -c dev-serve",
    "serve:prod": "ng serve -c production-serve",
    "build:local": "npm run build:lib:ngx-shared && ng build",
    "build:dev": "npm run build:lib:ngx-shared && ng build -c dev",
    "build:prod": "npm run build:lib:ngx-shared && ng build -c production",
    "build:lib:ngx-shared": "ng build ngx-shared",
    "build:lib:ngx-shared:watch": "ng build ngx-shared --watch",
    "publish:ngx-shared": "npm run build:lib:ngx-shared && npm publish ./dist/ngx-shared",
    "build:analyze": "npm run build:lib:ngx-shared && ng build --named-chunks --stats-json && webpack-bundle-analyzer dist/luv-coffee-fe/stats.json",
    "build:start": "npm run build:local && cd dist/luv-coffee-fe/browser && spa-server-gzip -p 4200",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:app": "ng lint luv-coffee-fe",
    "lint:lib:ngx-shared": "ng lint ngx-shared",
    "test": "ng test --watch=false",
    "test:app": "ng test luv-coffee-fe --watch=false",
    "test:app:watch": "ng test luv-coffee-fe",
    "test:lib:ngx-shared": "ng test ngx-shared --watch=false",
    "test:lib:ngx-shared:watch": "ng test ngx-shared",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --no-watch --code-coverage",
    "deploy:dev": "npm run build:dev && gcloud app deploy app.dev.yaml --project luv-coffee",
    "prepare": "husky"
  },
  "dependencies": {
    "@angular/animations": "^19.2.1",
    "@angular/cdk": "^19.2.2",
    "@angular/common": "^19.2.1",
    "@angular/compiler": "^19.2.1",
    "@angular/core": "^19.2.1",
    "@angular/forms": "^19.2.1",
    "@angular/material": "^19.2.2",
    "@angular/platform-browser": "^19.2.1",
    "@angular/platform-browser-dynamic": "^19.2.1",
    "@angular/router": "^19.2.1",
    "@ngnestpostgres/fe-shared": "^0.0.33",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular-eslint/builder": "^19.2.1",
    "@angular-eslint/eslint-plugin": "^19.2.1",
    "@angular-eslint/eslint-plugin-template": "^19.2.1",
    "@angular-eslint/schematics": "^19.2.1",
    "@angular-eslint/template-parser": "^19.2.1",
    "@angular/cli": "^19.2.1",
    "@angular/compiler-cli": "^19.2.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^50.6.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "typescript": "~5.8.2"
  }
}
